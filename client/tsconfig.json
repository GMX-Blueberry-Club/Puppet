{
  "compilerOptions": {
    "target": "ESNext",
    "types": [
      "vite-plugin-pwa/client",
    ],
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ESNext",
      "webworker"
    ],
    "module": "ESNext",
    "skipLibCheck": true,
    /* Bundler mode */
    "moduleResolution": "bundler",
    "allowImportingTsExtensions": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    /* Linting */
    "strict": true,
    // "noUnusedLocals": true,
    // "noUnusedParameters": true,
    "noFallthroughCasesInSwitch": true
  },
  "include": [
    "src",
  ],
  "references": [
    {
      "path": "../middleware"
    },
    {
      "path": "../const"
    },
    {
      "path": "../ui-components"
    },
    {
      "path": "../@gambitdao-gbc-middleware"
    },
  ]
}