{
  "compilerOptions": {
    "target": "ES2020",
    "lib": [
      "ES2020",
      "DOM",
      "DOM.Iterable",
      "WebWorker"
    ],
    "module": "ESNext",
    /* Bundler mode */
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "types": [
      "vite/client",
      "vite-plugin-pwa/vanillajs",
      "vite-plugin-pwa/info"
    ],
    "allowImportingTsExtensions": true,
    /* Linting */
    "strict": true,
    "noFallthroughCasesInSwitch": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "noEmit": true,
    "isolatedModules": true,
    "skipLibCheck": true
  },
  "references": [
    {
      "path": "../gmx-middleware/utils"
    },
    {
      "path": "../gmx-middleware/const"
    },
    {
      "path": "../middleware"
    },
    {
      "path": "../const"
    },
    {
      "path": "../common/utils"
    },
    {
      "path": "../common/ui-components"
    },
    {
      "path": "../common/ui-router"
    },
    {
      "path": "../common/wallet"
    },
    {
      "path": "../common/ui-storage"
    },
    {
      "path": "../@gambitdao-gbc-middleware"
    },
  ],
  "include": [
    "src"
, "src/sw.ts", "../utils/router", "../utils/indexer/scope", "../utils/indexer/process", "../common/wallet/src"  ]
}